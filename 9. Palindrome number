class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x < 0:
            return False
        else:
            length = x
            number_list = []
            count = 0
            while length >= 1:
                length /= 10
                count += 1
            for i in range(count):
                number_list.append(round(x % 10))
                x /= 10
            if number_list == number_list[::-1]:
                return True
            else:
                return False
